@model IEnumerable<string>

@{
    ViewData["Title"] = "Результати запиту";
    var s = ViewBag.Name;
}

<h1>Результати запиту</h1>

@if (Model.Count() > 0)
{
    <table class="table">
        <thead>
            <tr>
                <th>
                    Жанри автора  <b>@s</b>
                </th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item)
                    </td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>Результат запиту порожній</p>
}
<a asp-controller="Home" asp-action="Query1">До запитів</a>
<a asp-controller="Home" asp-action="Index">На головну сторінку</a>